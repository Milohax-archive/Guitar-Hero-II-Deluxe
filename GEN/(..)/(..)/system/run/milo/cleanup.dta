{dialog
   'Cleanup Wizard'
   {milo cur_dir}
   (editor
      ('Remove unused objects' bool)
      ('Reset bounding spheres' bool)
      ('Convert SrcAlpha to Src' bool)
      ('Cleanup all meshes' bool)
      ('   Make normals' bool)
      ('Collapse tex xfms' bool)
      ('Delete unused buffers' bool)
      ('Reset rendered textures' bool)
      ('Reset mutable meshes' bool)
      ('Order by blend' bool)
      ('Do cleanup'
         script
         (script
            {$this cleanup})))
   ('Remove unused objects' FALSE)
   ('Reset bounding spheres' FALSE)
   ('Convert SrcAlpha to Src' FALSE)
   ('Cleanup all meshes' FALSE)
   ('   Make normals' FALSE)
   ('Collapse tex xfms' FALSE)
   ('Delete unused buffers' FALSE)
   ('Reset rendered textures' FALSE)
   ('Reset mutable meshes' FALSE)
   ('Order by blend' FALSE)
   (cleanup
      {if
         {$this get 'Remove unused objects'}
         {milo remove_unused}}
      {if
         {$this get 'Reset bounding spheres'}
         {milo reset_spheres}}
      {if
         {$this get 'Convert SrcAlpha to Src'}
         {milo convert_srcalpha}}
      {if
         {$this get 'Cleanup all meshes'}
         {milo
            cleanup_meshes
            {$this get '   Make normals'}}}
      {if
         {$this get 'Collapse tex xfms'}
         {milo collapse_tex_xfms}}
      {if
         {$this get 'Delete unused buffers'}
         {milo delete_unused_bufs}}
      {if
         {$this get 'Reset rendered textures'}
         {milo reset_rendered_texs}}
      {if
         {$this get 'Reset mutable meshes'}
         {milo reset_mutable}}
      {if
         {$this get 'Order by blend'}
         {milo order_by_blend}}
      {milo reset_tool})}