(streaks
   (10 2)
   (20 3)
   (30 4))
; These all pertain to the crowd meter. Different difficulties have each
; note weighted differently to pull the crowd meter in one direction or
; another, because different difficulties have different note densities.
; This is also where we set no fail; setting lose_level to 0.0 means you'll
; never fail out of a song.
(default_config
   (crowd
      (kDifficultyEasy
         (note_weight 2.0e-2)
         (range 0.5 0.9)
         (free_ride 0.2)
         (warning_level 0.15)
         (lose_level
            {if_else $nofail
               0.0
               1.0e-2}))
      (kDifficultyMedium
         (note_weight 1.0e-2)
         (range 0.5 0.9)
         (free_ride 0.1)
         (warning_level 0.15)
         (lose_level
            {if_else $nofail
               0.0
               1.0e-2}))
      (kDifficultyHard
         (note_weight 8.0e-3)
         (range 0.6 0.9)
         (free_ride 0.0)
         (warning_level 0.15)
         (lose_level
            {if_else $nofail
                0.0
                1.0e-2}))
      (kDifficultyExpert
         (note_weight 8.0e-3)
         (range 0.65 0.9)
         (free_ride 0.0)
         (warning_level 0.15)
         (lose_level
            {if_else $nofail
                0.0
                1.0e-2})))
   (star_power
      (recharge_rate 0.0)
      (deploy_rate 0.125)
      (star_phrase 0.25)
      (whammy_rate 3.4e-2)
      (ready_level 0.5)
      (multiplier 2)
      (crowd_boost 6)
